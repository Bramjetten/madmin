<h1>New <%= resource.model.name.pluralize %></h1>

<% resource.attributes.each do |attribute| %>
  <% next if attribute[:field].nil? %>
  <% field = attribute[:field].new(record: @record, attribute_name: attribute[:name], partial: "form") %>

  <div><%= render partial: field.to_partial_path, locals: { field: field } %></div>
<% end %>
